{
	"name": "gatsby-plugin-paddle",
	"description": "Gatsby plugin that adds paddle.js to your site.",
	"version": "0.1.0",
	"author": "Ahmad Awais (https://twitter.com/MrAhmadAwais/)",
	"repository": {
		"type": "git",
		"url": "https://github.com/ahmadawais/gatsby-plugin-paddle.git"
	},
	"homepage": "https://github.com/ahmadawais/gatsby-plugin-paddle.git",
	"bugs": {
		"url": "https://github.com/ahmadawais/gatsby-plugin-paddle/issues"
	},
	"keywords": [
		"gatsby",
		"gatsby-plugin",
		"paddle",
		"paddle web gatsby",
		"paddle.js"
	],
	"license": "MIT",
	"scripts": {
		"build": "babel src --out-dir . --ignore __tests__",
		"start": "babel -w src --out-dir . --ignore __tests__",
		"watch": "babel -w src --out-dir . --ignore __tests__",
		"log": "auto-changelog && git add . && git ci -m ' ğŸ“– DOC: Changelog update' && git push",
		"release": "npm run build && release-it",
		"release-major": "npm run build && release-it major",
		"release-dry": "npm run build && release-it --dry-run"
	},
	"peerDependencies": {
		"gatsby": ">=1"
	},
	"devDependencies": {
		"release-it": "^7.6.3",
		"auto-changelog": "^1.9.0"
	},
	"dependencies": {
		"babel-cli": "6.26.0",
		"babel-preset-env": "1.7.0",
		"babel-preset-react": "6.24.1",
		"babel-runtime": "6.26.0",
		"cross-env": "5.2.0"
	}
}
